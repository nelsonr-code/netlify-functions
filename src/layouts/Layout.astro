---
import BaseHead from '@components/BaseHead.astro'

export interface Props {
  title: string
}

const { title, description } = Astro.props
---

<html lang="es">
  <head>
    <BaseHead
      title={`Netlify functions | ${title}`}
      description={description}
    />
  </head>
  <body>
    <main>
      <slot />
    </main>
  </body>
</html>
